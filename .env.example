# Server settings
CARPENTER_DEBUG=false
CARPENTER_ENVIRONMENT=development
CARPENTER_SECRET_KEY=your-secret-key-here
CARPENTER_ALLOWED_HOSTS=localhost,127.0.0.1
CARPENTER_DATABASE_URL=postgres://user:password@localhost:5432/carpenter
CARPENTER_PORT=8000
CARPENTER_HOST=127.0.0.1
CARPENTER_CMD=dev

# Frontend settings
CARPENTER_VITE_PORT=3000
CARPENTER_PACKAGE_MANAGER=npm
CARPENTER_FRONTEND_COMMAND=dev

# Logging
CARPENTER_LOG_LEVEL=error
CARPENTER_ENV_FILE=.env

# Authentication
CARPENTER_AUTH_REQUIRED=true